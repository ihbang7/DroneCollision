  }
};
  
  
var data = [drone0, drone1,drone2, drone3];
// Replace the code below using the ones produced from Java code (FINISH)

var layout = {
  title: '3D Line Plot',
  autosize: false,
  width: 500,
  height: 500,
  margin: {
    l: 0,
    r: 0,
    b: 0,
    t: 65
  }
};
Plotly.newPlot('myDiv', data, layout); 
});
