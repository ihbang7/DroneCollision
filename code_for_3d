    size: 20
  }
};
var drone3 = {  // How to modify 'trace' into 'drone'?
  selected_color: getRandomColor(),
  x: unpack(rows, 'x3'),
  y: unpack(rows, 'y3'),
  z: unpack(rows, 'z3'),
  mode: 'lines+markers+text',
  marker: {
    color: selected_color,
    size: 12,
